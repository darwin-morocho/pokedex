{
    "editor.codeActionsOnSave": {
        "source.fixAll": true,
        "source.organizeImports": true,
    }
    ,
    "dart.previewFlutterUiGuides": true,
    "command-runner.commands": {
      "build_runner delete-conflicting-outputs": "flutter pub run build_runner build --delete-conflicting-outputs",
      "build_runner": "flutter pub run build_runner build",
      "flutter pub get": "flutter pub get",
      "flutter clean": "flutter clean",
      "flutter_gen": "flutter pub run flutter_gen:flutter_gen_command",
      "lcov genthml": "genhtml coverage/lcov.info --output=coverage"
     }
}